Package: pacman
Type: Package
Title: Package Management Tool
Version: 0.4.7
Date: 2017-05-13
Authors@R: c(person("Tyler", "Rinker", role = c("aut", "cre", "ctb"), email =
         "tyler.rinker@gmail.com"), person("Dason", "Kurkiewicz", role = c("aut",
         "ctb"), email = "dasonk@gmail.com"), person("Keith", "Hughitt", role =
         c("ctb")), person("Albert", "Wang", role = c("ctb")), person("Jim",
